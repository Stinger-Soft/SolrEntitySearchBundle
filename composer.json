{
	"name" : "stinger-soft/solr-entity-search-bundle",
	"type" : "symfony-bundle",
	"description" : "Solr Layer to index and search entities",
	"keywords" : [
		"Search"
	],
	"homepage" : "http://www.stinger-soft.net",
	"authors" : [{
			"name" : "Florian Meyer",
			"email" : "florian.meyer@stinger-soft.net",
			"role" : "Developer"
		}
	],
	"autoload" : {
		"psr-4" : {
			"StingerSoft\\SolrEntitySearchBundle\\" : ""
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"license" : "Apache-2.0",
	"require" : {
		"php" : ">=5.5",
		"stinger-soft/entity-search-bundle" : "~0.1.0@dev",
		"knplabs/knp-paginator-bundle" : "~2.5",
		"stinger-soft/php-commons" : "^0.1@dev",
		"solarium/solarium" : "~3.6.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4.1|~5.0",
		"satooshi/php-coveralls" : "~0.7.1",
		"doctrine/orm" : ">=2.5.0",
		"doctrine/common" : ">=2.5.0",
		"symfony/phpunit-bridge" : "^2.8.1|^3.0.1"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "0.1.x-dev",
			"dev-develop" : "1.0.x-dev"
		}
	}
}